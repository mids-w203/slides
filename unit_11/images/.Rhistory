library(ggplot2)
library(tidyverse)
theme_set(theme_minimal(base_size = 20))
set.seed(1)
m <- data.frame(
monorail = factor(
rep(0:1, times = c(12, 6)),
labels = c("No Monorail","Monorail")
)) %>%
mutate(
visitors = (400 + (3000 * I(monorail == 'Monorail'))) * rexp(18)) %>%
mutate(
visitors = as.numeric(visitors / 100)
)
m
f <- data.frame(
fish = factor(
rep(0:1, times = c(400, 50)),
labels = c('Less than 500', 'More than 500')
)) %>%
mutate(
visitors = as.numeric(
5 * (1 + 10 * I(fish == 'More than 500')) * rexp(450, rate = 0.1)
))
f %>%
ggplot(aes(x = fish, y = visitors, fill = fish)) +
geom_boxplot() +
geom_jitter(width = 0.1, alpha = 0.5) +
scale_fill_manual(values = c('#003262', '#FDB515')) +
labs(
y = 'Visitors (in thousands)',
x = 'Dumpsters',
title = 'Dumpsters Increase Visitors',
legend = NULL
) +
theme(legend.position = 'none')
